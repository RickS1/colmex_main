
<% anio = @actas.order(anio: :desc).first.anio %>
<table>
<% @actas.where(:anio => anio).each do |d| %>

<tr>
	<td>
		<a href=<%= d.liga.to_s == "" ? d.archivo.url : d.liga %> target="_blank" class="documento uk-link"><img src=<%= image_url("documento.png") %> alt="documento" class="documento uk-vertical-align-middle"></a>
	</td>
	<td>
		<a href=<%= d.liga.to_s == "" ? d.archivo.url : d.liga %> target="_blank" class="documento uk-link"><%= d.nombre %></a>
	</td>
</tr>
<% end %>
</table>
